<?xml version="1.0" encoding="UTF-8"?>
<Floating widthPt="100" heightPt="100">
	<HBox
		widthPt="100"
		heightPt="100"
		skin:Paint-color="0xC0C0C0"
		skin:Paint-border="1"
		skin:Paint-borderColor="0x000000"
		layout:Column-cols="[-1, 45, 65]">
		<InputText
			name="'nameInp'"
			widthPt="100"
			heightPt="100"
			text="''"
			format.align="CENTER"
			align="'middle'"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000" />
		<Button
			name="'okBtn'"
			heightPt="100"
			tabEnabled="false"
			label-visible="true"
			text="'Ok'"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000" />
		<Button
			name="'cancelBtn'"
			heightPt="100"
			tabEnabled="false"
			label-visible="true"
			text="'Cancel'"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000" />
	</HBox>
</Floating>
